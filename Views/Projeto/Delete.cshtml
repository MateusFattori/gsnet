@model gsnet.Models.Projeto

<h1>Excluir Projeto</h1>

<h3>Tem certeza de que deseja excluir este projeto?</h3>
<div>
    <h4>Nome</h4>
    <h5>@Html.DisplayFor(model => model.Nome)</h5>
    <h4>Data de Início</h4>
    <h5>@Html.DisplayFor(model => model.dt_inicio)</h5>
    <h4>Data Final</h4>
    <h5>@Html.DisplayFor(model => model.dt_final)</h5>
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Excluir</button> |
    <a asp-action="Index">Cancelar</a>
</form>
