@model gsnet.Models.Projeto

<h1>Criar Projeto</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Nome" class="control-label"></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="dt_inicio" class="control-label"></label>
        <input asp-for="dt_inicio" class="form-control" />
        <span asp-validation-for="dt_inicio" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="dt_final" class="control-label"></label>
        <input asp-for="dt_final" class="form-control" />
        <span asp-validation-for="dt_final" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Criar" class="btn btn-primary" />
    </div>
</form>
